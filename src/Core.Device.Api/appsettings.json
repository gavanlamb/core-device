{
  "AllowedHosts": "*",
  "DeviceDatabaseSettings": {
    "DeviceCollectionName": "Devices",
    "TypeCollectionName": "Types",
    "DatabaseName": "Device"
  },
  "Serilog": {
    "Using": [ 
      "Serilog.Sinks.Console" 
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      { "Name": "Console" }
    ]
  }
}
